<!doctype html>
<html>
  <head>
    <title>Movie Search Results</title>
    <link rel="stylesheet" type="text/css" href="/style/style.css">
  </head>
  <body>
    <ul>
    	<% movieList.forEach(function(movie, index){ %>
    	<li><div class=list><a href="movie/<%= movie.imdbID %>"><%= movie.Title %></a></div></li>
		<% }) %>
    </ul>
  </body>
</html>
